[FUNCTION f_com]
IF (<ARGS>)
Say @0790 Comeca em <dARGS> Segundos.
timerf 1 f_COM <EVAL <ARGS>-1>
ELSE
MORE2 1
TIMER 1
RETURN 1
ENDIF

