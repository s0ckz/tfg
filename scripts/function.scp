	IF (<TOTALGOLD> < <ARGN1>)
		RETURN 1
	ENDIF
	BOUNCE <FINDLAYER.layer_dragging>
	LOCAL.AMOUNT=<ARGN1>

	WHILE (<LOCAL.AMOUNT>)
		IF (0<FINDLAYER.21.FINDID.i_gold>)
			LOCAL.GOLD=<FINDLAYER.21.FINDID.i_gold.amount>
